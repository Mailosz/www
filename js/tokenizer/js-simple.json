{
	"default":{
		"begin": [{
			"on": ["controlSymbols", "literalEnd"]
		},{
			"on": "word-break",
			"by": {"regex": "\\w"}
		},{
			"on": "lineComment",
			"by": ["\n"]
		},{
			"on": "commentEnd",
			"when": {
				"commentLocation": "everywhere"
			}
		}],
		"default": true,
		"then": {
			"literalType": "\"",
			"commentLocation": "everywhere"
		},
		"after": [{
			"by": ["var", "let", "const", "if", "for", "while", "function", "this", "instanceof", "throw", "catch", "return", "break", "continue", "null", "export", "class", "try", "await", "async", "static"],
			"data": {
				"class": "keyword"
			}
		}]
	}, "word-break": {
		"begin": [{
			"on": ["default"],
			"by": {"regex": "\\s"}
		}]
	}, "controlSymbols": {
		"begin": [{
			"on": ["default", "word-break"],
			"by": ["=", "!", "{", "}", "(", ")", "[", "]", "+", "-", "==", "=>", ":", ";"]
		}]
	}, "literalStart": {
		"begin": [{
			"on": ["default"],
			"by": ["\""],
			"then": {
				"literalType": "\""
			}
		},{
			"on": ["default"],
			"by": ["'"],
			"then": {
				"literalType": "'"
			}
		},{
			"on": ["default"],
			"by": ["`"],
			"then": {
				"literalType": "`"
			}
		}]
	}, "literal": {
		"begin": [{
			"on": ["literalStart", "literalSpecialchar"]
		},{
			"on": ["commentEnd"],
			"when": {
				"commentLocation": "literal"
			}
		}]
	}, "literalSpecialchar": {
		"begin": [{
			"on": ["literal"],
			"by": ["\\\""],
			"when": {
				"literalType": "\""
			}
		},{
			"on": ["literal"],
			"by": ["\\'"],
			"when": {
				"literalType": "'"
			}
		},{
			"on": ["literal"],
			"by": ["\\`"],
			"when": {
				"literalType": "`"
			}
		}]
	}, "literalEnd": {
		"begin": [{
			"on": ["literal"],
			"by": ["\""],
			"when": {
				"literalType": "\""
			}
		},{
			"on": ["literal"],
			"by": ["'"],
			"when": {
				"literalType": "'"
			}
		},{
			"on": ["literal"],
			"by": ["`"],
			"when": {
				"literalType": "`"
			}
		}]
	}, "lineComment": {
		"begin": [{
			"everywhere": true,
			"by": ["//"]
		}]
	}, "commentStart": {
		"begin": [{
			"everywhere": true,
			"by": "/*",
			"then": {
				"commentLocation": "everywhere"
			}
		},{
			"on": ["literal"],
			"by": "/*",
			"then": {
				"commentLocation": "literal"
			}
		}]
	}, "comment": {
		"begin": {
			"on": "commentStart"
		}
	}, "commentEnd": {
		"begin": {
			"on": "comment",
			"by": "*/"
		}
	}

}